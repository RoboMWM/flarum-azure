<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>

		<rule name="rule 1u">
			<match url=".*"  />
			<action type="Rewrite" url="/-"  />
		</rule>
		<rule name="rule 2u" stopProcessing="true">
			<match url="^vendor/(.*)?$"  />
			<action type="Rewrite" url="//"  />
		</rule>
		<rule name="rule 3u" stopProcessing="true">
			<match url="^storage/(.*)?$"  />
			<action type="Rewrite" url="//"  />
		</rule>
		<rule name="rule 4u" stopProcessing="true">
			<match url="^config.php$"  />
			<action type="Rewrite" url="//"  />
		</rule>
		<rule name="rule 5u" stopProcessing="true">
			<match url="^api(.*)$"  />
			<action type="Rewrite" url="/api.php"  appendQueryString="true" />
		</rule>
		<rule name="rule 6u" stopProcessing="true">
			<match url="^admin(.*)$"  />
			<action type="Rewrite" url="/admin.php"  appendQueryString="true" />
		</rule>
		<rule name="rule 7u" stopProcessing="true">
			<match url="!^assets"  />
			<action type="Rewrite" url="/index.php"  appendQueryString="true" />
		</rule>
		
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
